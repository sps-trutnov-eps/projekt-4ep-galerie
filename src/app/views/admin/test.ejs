<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <title>Document</title>
</head>
<body>
    <h2 class="m-4">STATS</h2>
    <div class="m-4">
        <p>Počet článků: <strong><%= Object.keys(data).length %></strong></p>
    </div>
    <table>
        
    </table>
    <% for(var i = 0; i < Object.keys(data).length; i++) { %>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th scope="col"><%= Object.keys(data)[i] %></th>
                <th scope="col">DATA</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <th scope="col">AUTOR</th>
                <td><%= JSON.stringify(Object.values(Object.entries(data)[i][1].autor)) %></td>
            </tr>
            <tr>
                <th scope="col">DATUM</th>
                <td><%= Object.entries(data)[i][1].datum %></td>
            </tr>
            <tr>
                <th scope="col">VIDITELNY</th>
                <td><%= Object.entries(data)[i][1].viditelny %></td>
            </tr>
            <tr>
                <th scope="col">NADPIS</th>
                <td><%= Object.entries(data)[i][1].nadpis %></td>
            </tr>
            <tr>
                <th scope="col">POPIS_SHORT</th>
                <td><%= Object.entries(data)[i][1].popis_short %></td>
            </tr>
            <tr>
                <th scope="col">POPIS_FULL</th>
                <td><%= Object.entries(data)[i][1].popis_full %></td>
            </tr>
            <tr>
                <th scope="col">TAGY</th>
                <td><%= Object.entries(data)[i][1].tagy %></td>
            </tr>
            <tr>
                <th scope="col">RECENZE</th>
                <td><%= Object.entries(data)[i][1].recenze %></td>
            </tr>
        </tbody>
    </table>
    <% } %>
    
    <div class="m-4">
        <p><strong>full: </strong><code><%= JSON.stringify(data, null, 4) %></code></p>
    </div>
    
</body>
</html>